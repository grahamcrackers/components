{
    "name": "@grahamcrackers/components",
    "version": "0.1.1",
    "description": "A Component Library",
    "author": "Graham Rogers",
    "license": "MIT",
    "type": "module",
    "sideEffects": [
        "dist/*",
        "dist/index.css",
        "tailwind.config.ts"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "dev": "tsc -w",
        "build": "yarn tsx ./scripts/build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prepare": "husky",
        "commit": "git-cz"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.6.0",
        "@radix-ui/react-accordion": "^1.2.0",
        "@radix-ui/react-alert-dialog": "^1.1.1",
        "@radix-ui/react-aspect-ratio": "^1.1.0",
        "@radix-ui/react-avatar": "^1.1.0",
        "@radix-ui/react-checkbox": "^1.1.0",
        "@radix-ui/react-collapsible": "^1.1.0",
        "@radix-ui/react-context-menu": "^2.2.1",
        "@radix-ui/react-dialog": "^1.1.1",
        "@radix-ui/react-dropdown-menu": "^2.1.1",
        "@radix-ui/react-hover-card": "^1.1.1",
        "@radix-ui/react-icons": "^1.3.0",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-menubar": "^1.1.1",
        "@radix-ui/react-navigation-menu": "^1.2.0",
        "@radix-ui/react-popover": "^1.1.1",
        "@radix-ui/react-progress": "^1.1.0",
        "@radix-ui/react-radio-group": "^1.2.0",
        "@radix-ui/react-scroll-area": "^1.1.0",
        "@radix-ui/react-select": "^2.1.1",
        "@radix-ui/react-separator": "^1.1.0",
        "@radix-ui/react-slider": "^1.2.0",
        "@radix-ui/react-slot": "^1.1.0",
        "@radix-ui/react-switch": "^1.1.0",
        "@radix-ui/react-tabs": "^1.1.0",
        "@radix-ui/react-toast": "^1.2.1",
        "@radix-ui/react-toggle": "^1.1.0",
        "@radix-ui/react-toggle-group": "^1.1.0",
        "@radix-ui/react-tooltip": "^1.1.1",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "cmdk": "^1.0.0",
        "date-fns": "^3.6.0",
        "embla-carousel-react": "^8.1.5",
        "input-otp": "^1.2.4",
        "lucide-react": "^0.396.0",
        "next-themes": "^0.3.0",
        "react-day-picker": "^8.10.1",
        "react-hook-form": "^7.52.0",
        "react-resizable-panels": "^2.0.19",
        "sonner": "^1.5.0",
        "tailwind-merge": "^2.3.0",
        "tailwindcss-animate": "^1.0.7",
        "vaul": "^0.9.1",
        "zod": "^3.23.8"
    },
    "peerDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.6",
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
        "@types/node": "^20.14.8",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.13.1",
        "@typescript-eslint/parser": "^7.13.1",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.3",
        "eslint-plugin-react-hooks": "^4.6.2",
        "fast-glob": "^3.3.2",
        "fs-extra": "^11.2.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.7",
        "postcss": "^8.4.38",
        "prettier": "^3.3.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "release-it": "^17.4.0",
        "tailwindcss": "^3.4.4",
        "tsup": "^6.0.1",
        "tsx": "^4.15.7",
        "typescript": "^5.5.2"
    },
    "lint-staged": {
        "./{src,tests}/**/*.{ts,tsx}": [
            "yarn lint --fix"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "types": "./dist/index.d.ts",
    "exports": {
        "./index.css": "./dist/index.css",
        "./tailwind.config": "./dist/tailwind.config.ts",
        "./contexts": {
            "require": "./dist/components/contexts/index.cjs",
            "import": "./dist/components/contexts/index.js"
        },
        "./alert-dialog": {
            "require": "./dist/components/alert-dialog/index.cjs",
            "import": "./dist/components/alert-dialog/index.js",
            "types": "./dist/components/alert-dialog/index.d.ts"
        },
        "./button": {
            "require": "./dist/components/button/index.cjs",
            "import": "./dist/components/button/index.js",
            "types": "./dist/components/button/index.d.ts"
        },
        "./card": {
            "require": "./dist/components/card/index.cjs",
            "import": "./dist/components/card/index.js",
            "types": "./dist/components/card/index.d.ts"
        }
    }
}
